{% extends "layout.html" %}

{% block title %}{{ super() }} | Blog{% endblock %}
{% block meta %}
<meta name="description" content="{{ self.title() }}" />
<link rel="canonical" href="{{ origin_dd ~ page.data.path }}" />
{% endblock %}

{% block main %}
<ul>
  {% for article in page.articles %}
  <li><a href="{{ article.full_slug }}">{{ article.content.title }}</a></li>
  {% endfor %}
</ul>
{% endblock %}