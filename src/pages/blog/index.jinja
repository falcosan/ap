{% extends "layout.html" %}

{% block head %}
<title>AP | Blog</title>
<meta name="description" content="AP | Blog" />
<link rel="canonical" href="{{ origin_dd ~ page.data.path }}" />
{% endblock %}
{% block main %}
<ul>
  {% for article in page.articles %}
  <li><a href="{{ article.full_slug }}">{{ article.content.title }}</a></li>
  {% endfor %}
</ul>
{% endblock %}